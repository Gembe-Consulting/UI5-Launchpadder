{
	"_version": "1.2.0",
    "start_url": "index.html",

	"sap.app": {
		"_version": "1.2.0",
		"id": "launchpadder",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{app.Title}}",
        "subTitle": "{{app.SubTitle}}",
		"description": "{{app.Description}}",
		"tags": {
            "keywords": [
                "{{app.Keyword1}}", "{{app.Keyword2}}"
            ]
    },
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"mainService": {
				"uri": "http://localhost:1337/services.odata.org/V2/Northwind/Northwind.svc/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri" : "localService/metadata.xml"
				}
			}
		}
	},

	"sap.ui": {
		"_version": "1.2.0",
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
            "sap_belize",
			"sap_bluecrystal"
		]
	},

	"sap.ui5": {
		"_version": "1.1.0",
		"rootView": {
			"viewName": "launchpadder.view.App",
			"type": "XML",
			"id": "rootApp"
		},

		"dependencies": {
			"minUI5Version": "1.38.0",
			"libs": {
                "sap.ui.unified": {},
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},

		"contentDensities": {
			"compact": true,
			"cozy": true
		},

		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "launchpadder.i18n.i18n"
				}
			},
            "shellModel": {
				"type": "sap.ui.model.json.JSONModel"
			},
            "userModel": {
				"type": "sap.ui.model.json.JSONModel"
			}
		},

		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "launchpadder.view",
				"controlId": "launchpadderShell",
				"controlAggregation": "content",
				"clearControlAggregation":true,
				"transition": "slide",
				"bypassed": {
					"target": "notFound"
				},
				"async": true
			},

			"routes": [
				{
					"pattern": "",
					"name": "launchpad",
					"target": "launchpad"
				}
			],

			"targets": {
				"launchpad": {
					"viewName": "AppTileContainer",
					"viewId": "appTileContainer",
					"viewLevel": 1,
					"async": false
				},
				"notFound": {
					"viewName": "NotFound",
					"viewId": "notFound",
					"transition": "show"
				}
			}
		}
	}
}
