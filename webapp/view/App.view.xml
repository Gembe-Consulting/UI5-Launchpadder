<?xml version="1.0"?>
<mvc:View xmlns="sap.ui.unified" xmlns:mvc="sap.ui.core.mvc" xmlns:m="sap.m" xmlns:c="sap.ui.core" controllerName="launchpadder.controller.App" displayBlock="true" width="100%" height="100%">


				<Shell id="launchpadderShell" icon="{shellModel>/logo}">
				
					<headItems>
						<ShellHeadItem icon="sap-icon://home" startsSection="true" press="onNavToLaunchpad"/>
					</headItems>
					
					<headEndItems>
						<ShellHeadItem icon="sap-icon://log" startsSection="true"/>
					</headEndItems>
					
					<search>
						<m:SearchField placeholder="{i18n>shell.searchFieldPlaceholder}" showRefreshButton="true"/>
					</search>
					
					<user>
						<ShellHeadUserItem username="{userModel>/userFullName}" image="{userModel>/userImage}"/>
					</user>
					
					<content>
						
					</content>
					
				</Shell>

</mvc:View>
